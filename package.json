{
  "name": "botmin",
  "version": "1.0.0",
  "description": "discord admin as a bot",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix",
    "dev": "nodemon --require dotenv/config --exec babel-node --extensions '.ts' src/index.ts",
    "build:clean": "rm -rf dist/ && mkdir dist",
    "build:bot": "babel src --out-dir dist/ --extensions '.ts'",
    "build": "npm run build:clean && npm run build:bot",
    "serve": "NODE_ENV=production node dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/samantharachelb/discord-blocklist.git"
  },
  "author": "samantha belnavis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samantharachelb/discord-blocklist/issues"
  },
  "homepage": "https://github.com/samantharachelb/discord-blocklist#readme",
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "@typeit/discord": "^4.0.9",
    "dd-trace": "^0.30.5",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "fs": "0.0.1-security",
    "fs-extra-promise": "^1.0.1",
    "hot-shots": "^8.2.0",
    "json-query": "^2.2.2",
    "moment": "^2.29.1",
    "mongoose": "^5.11.13",
    "reflect-metadata": "^0.1.13",
    "toml": "^3.0.0",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3",
    "uuid": "^8.3.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@types/node": "^14.14.22",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-static-fs": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^7.18.0",
    "nodemon": "^2.0.6",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3"
  }
}
